<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".config.WmpApplication"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/icon_launcher_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.WeMakePass"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <activity
            android:name=".view.board.BoardActivity"
            android:exported="false" />
        <activity
            android:name=".view.board.search.BoardSearchActivity"
            android:exported="false" />
        <activity
            android:name=".view.exam.ExamActivity"
            android:exported="false" />
        <activity
            android:name=".view.main.home.interestJm.InterestJmSearchActivity"
            android:exported="false" />
        <activity
            android:name=".view.accountSetting.AccountSettingActivity"
            android:exported="false" />
        <activity
            android:name=".view.splash.SplashActivity"
            android:exported="false"
            android:theme="@style/Theme.Splash" />
        <activity
            android:name=".view.auth.findAccount.FindAccountActivity"
            android:exported="false" />
        <activity
            android:name=".view.auth.AuthActivity"
            android:exported="false" />
        <activity
            android:name=".view.main.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>